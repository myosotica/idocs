  "homepage": "https://github.com/iconicompany/idocs#readme",
  "dependencies": {
    "next": "^13.2.4",
    "nextra": "2.2.19",
    "nextra-theme-docs": "2.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
